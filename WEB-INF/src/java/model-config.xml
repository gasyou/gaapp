<?xml version="1.0" encoding="UTF-8"?>
<model-config>
	<model name="Login">
		<model-class value="login.model.LoginModel"></model-class>
		<forward name="success" path="/WEB-INF/jsp/login.jsp"></forward>
	</model>
	<model name="UserList">
		<model-class value="user.model.UserListModel"></model-class>
		<forward name="success" path="/WEB-INF/jsp/userList.jsp"></forward>
	</model>
	<model name="User">
		<model-class value="user.model.UserModel"></model-class>
		<forward name="success" path="/WEB-INF/jsp/user.jsp"></forward>
	</model>
	<model name="UserUpdate">
		<model-class value="user.model.UserUpdateModel"></model-class>
		<forward name="success" path="/WEB-INF/jsp/complete.jsp"></forward>
		<forward name="confirm_success" path="/WEB-INF/jsp/user.jsp"></forward>
	</model>
</model-config>